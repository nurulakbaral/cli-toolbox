{
  "name": "@cli-toolbox/project",
  "version": "0.0.2",
  "description": "CLI toolbox that focuses on setting up projects, creating apps, managing coding styles, adding dependencies, and other project initialization tasks",
  "main": "dist/index.js",
  "scripts": {
    "lib:build": "npx rimraf dist && rollup --config=rollup.config.ts --configPlugin=@rollup/plugin-typescript",
    "lib:tests": "vitest --reporter=verbose",
    "lib:publish": "npm publish ./dist --access public",
    "lib:publish-local": "npm publish ./dist --access public --registry http://localhost:4873"
  },
  "type": "module",
  "keywords": [
    "generator",
    "cli",
    "automation",
    "plop",
    "scaffolding"
  ],
  "author": "nurulakbaral@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nurulakbaral/cli-toolbox.git"
  },
  "bugs": {
    "url": "https://github.com/nurulakbaral/cli-toolbox/issues"
  }
}
